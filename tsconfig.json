{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/utils/*": [
        "utils/*"
      ],
      "@/interfaces/*": [
        "interfaces/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/contexts/*": [
        "contexts/*"
      ],
      "@/icons/*": [
        "icons/*"
      ],
      "@/languages/*": [
        "languages/*"
      ],
      "@/root/*": [
        "*"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/api/auth/[...nextauth].ts",
    "next.config.js",
    "tailwind.config.js",
    "utils.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
